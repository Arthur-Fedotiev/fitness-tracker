{
  "name": "fitness-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build --stats-json",
    "local-function": "firebase emulators:start --only functions",
    "local-dev": "firebase emulators:start --only firestore,functions --import sample-exercises",
    "empty-emulator": "firebase-emulators:start",
    "export-emulator": "firebase emulators:export sample-exercises",
    "emulator": "firebase emulators:start --only=firestore,functions,auth --import=emulated-samples --export-on-exit",
    "analyze:fitness-tracker": "webpack-bundle-analyzer dist/apps/fitness-tracker/stats.json",
    "update-locale": "ngx-translate-extract -i ./src -o src/assets/locale/*.json --sort --format namespaced-json",
    "serve:prod": "nx build && firebase serve --only hosting -p 3000",
    "cypress:run": "cd apps/fitness-tracker-e2e/ && cypress run",
    "cypress:open": "cd apps/fitness-tracker-e2e/ && cypress open",
    "deploy:staging": "firebase use staging && nx run fitness-tracker:build:staging && firebase deploy --only hosting",
    "deploy-functions:staging": "firebase use staging && firebase deploy --only functions",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ddd": "^2.1.2",
    "@angular/animations": "14.2.2",
    "@angular/cdk": "14.2.2",
    "@angular/common": "14.2.2",
    "@angular/compiler": "14.2.2",
    "@angular/core": "14.2.2",
    "@angular/fire": "^7.4.0",
    "@angular/flex-layout": "^14.0.0-beta.41",
    "@angular/forms": "14.2.2",
    "@angular/material": "14.2.2",
    "@angular/platform-browser": "14.2.2",
    "@angular/platform-browser-dynamic": "14.2.2",
    "@angular/pwa": "^14.2.6",
    "@angular/router": "14.2.2",
    "@angular/service-worker": "14.2.2",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@firebase/app": "^0.7.33",
    "@ng-web-apis/common": "^2.0.1",
    "@ngneat/until-destroy": "9.0.0",
    "@ngrx/component-store": "14.0.2",
    "@ngrx/effects": "14.0.2",
    "@ngrx/entity": "14.0.2",
    "@ngrx/router-store": "14.0.2",
    "@ngrx/store": "14.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@nrwl/angular": "14.7.8",
    "algoliasearch": "^4.14.2",
    "angular-instantsearch": "^4.2.1",
    "firebase": "^9.4.0",
    "firebaseui": "^6.0.0",
