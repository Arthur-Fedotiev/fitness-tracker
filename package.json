{
  "name": "fitness-tracker",
  "version": "0.0.0",
  "license": "MIT",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "start": "nx serve",
    "build": "nx build --stats-json",
    "local-function": "firebase emulators:start --only functions",
    "local-dev": "firebase emulators:start --only firestore,functions --import sample-exercises",
    "empty-emulator": "firebase-emulators:start",
    "export-emulator": "firebase emulators:export sample-exercises",
    "emulator": "firebase emulators:start --only=firestore,functions,auth --import=emulated-samples --export-on-exit",
    "analyze:fitness-tracker": "webpack-bundle-analyzer dist/apps/fitness-tracker/stats.json",
    "update-locale": "ngx-translate-extract -i ./src -o src/assets/locale/*.json --sort --format namespaced-json",
    "serve:prod": "nx build && firebase serve --only hosting -p 3000",
    "cypress:run": "cd apps/fitness-tracker-e2e/ && cypress run",
    "cypress:open": "cd apps/fitness-tracker-e2e/ && cypress open",
    "deploy:staging": "firebase use staging && nx run fitness-tracker:build:staging && firebase deploy --only hosting",
    "deploy:prod": "firebase use prod && nx run fitness-tracker:build:production && firebase deploy --only hosting",
    "deploy-functions:staging": "firebase use staging && firebase deploy --only functions",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/ddd": "^2.1.2",
    "@angular/animations": "20.1.4",
    "@angular/cdk": "20.1.4",
    "@angular/common": "20.1.4",
    "@angular/compiler": "20.1.4",
    "@angular/core": "20.1.4",
    "@angular/fire": "^18.0.1",
    "@angular/forms": "20.1.4",
    "@angular/material": "20.1.4",
    "@angular/platform-browser": "20.1.4",
    "@angular/platform-browser-dynamic": "20.1.4",
    "@angular/pwa": "20.1.4",
    "@angular/router": "20.1.4",
    "@angular/service-worker": "20.1.4",
    "@biesbjerg/ngx-translate-extract": "7.0.4",
    "@ng-web-apis/common": "^3.0.1",
    "@ngneat/until-destroy": "9.0.0",
    "@ngrx/component-store": "^20.0.0",
    "@ngrx/effects": "^20.0.0",
    "@ngrx/entity": "^20.0.0",
    "@ngrx/operators": "^18.0.0",
    "@ngrx/router-store": "^20.0.0",
    "@ngrx/store": "^20.0.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@nx/angular": "21.3.11",
    "firebase": "^9.23.0",
    "lodash-es": "^4.17.21",
    "rxfire": "^6.0.3",
    "rxjs": "7.8.1",
    "shared-package": "file:shared-package-5.5.0.tgz",
    "tslib": "^2.3.0",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.1.4",
    "@angular-devkit/core": "20.1.4",
    "@angular-devkit/schematics": "20.1.4",
    "@angular-eslint/eslint-plugin": "17.3.0",
    "@angular-eslint/eslint-plugin-template": "17.3.0",
    "@angular-eslint/template-parser": "17.3.0",
    "@angular/cli": "~20.1.0",
    "@angular/compiler-cli": "20.1.4",
    "@angular/language-service": "20.1.4",
    "@hirez_io/observer-spy": "^2.2.0",
    "@ngrx/schematics": "19.2.1",
    "@ngrx/store-devtools": "^20.0.0",
    "@nx/cypress": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "21.3.11",
    "@nx/workspace": "21.3.11",
    "@schematics/angular": "20.1.4",
    "@types/jest": "30.0.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "18.19.99",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cypress": "^12.2.0",
    "cypress-firebase": "^2.2.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "firebase-admin": "^11.2.0",
    "jasmine-marbles": "~0.9.1",
    "jest": "29.7.0",
