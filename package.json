    "@nx/workspace": "16.4.2",
    "@schematics/angular": "16.1.3",
    "@types/jest": "29.4.4",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.14.33",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cypress": "^12.2.0",
