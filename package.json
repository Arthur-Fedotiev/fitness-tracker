    "nx": "21.3.11",
    "prettier": "2.8.1",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.1",
    "typescript": "5.8.2",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
