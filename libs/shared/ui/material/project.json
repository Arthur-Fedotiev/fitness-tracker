{
  "name": "shared-ui-material",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/ui/material/src",
  "selector": "ft",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/shared/ui/material"],
      "options": {
        "jestConfig": "libs/shared/ui/material/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/ui/material/src/**/*.ts",
          "libs/shared/ui/material/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:shared", "type:ui"]
}
