{
  "name": "workout-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/workout/ui/src",
  "prefix": "ft",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/workout/ui"],
      "options": {
        "jestConfig": "libs/workout/ui/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/workout/ui/src/**/*.ts",
          "libs/workout/ui/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:workout", "type:ui"]
}
