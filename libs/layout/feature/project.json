{
  "name": "layout-feature",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/layout/feature/src",
  "selector": "ft",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/layout/feature"],
      "options": {
        "jestConfig": "libs/layout/feature/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/layout/feature/src/**/*.ts",
          "libs/layout/feature/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["domain:layout", "type:feature"]
}
